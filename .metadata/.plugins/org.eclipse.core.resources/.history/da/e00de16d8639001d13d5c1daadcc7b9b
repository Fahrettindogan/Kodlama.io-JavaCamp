package miniProjeAsalSayi;

public class Main {

	public static void main(String[] args) {

		int number = 17;
		//int remainder = number % 2;
		//System.out.println(remainder);
		
		for(int i=2;i<number;i++) {
			if(number % i == 0) {
				System.out.println("Sayi asal degildir.");
				break;
			}
			
		}
	}

}
