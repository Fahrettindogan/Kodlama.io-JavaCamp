package loopDemo;

public class Main {

	public static void main(String[] args) {
		//for
		for(int i=2;i<10;i=i+2) {
			System.out.println(i);
		}
		System.out.println("For Dongusu Bitti.");
		int i=1;
		//while
		while(i<10) {
			System.out.println(i);
			i++;
		}
		System.out.println("While Dongusu Bitti.");
	}
}
